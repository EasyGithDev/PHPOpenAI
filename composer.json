{
    "name": "easygithdev/php-openai",
    "description": "PHPOpenAI is a wrapper to access OpenAI API",
    "keywords": [
        "php",
        "ai",
        "openai",
        "dalle",
        "dall-e",
        "gpt-3",
        "gpt-3.5",
        "gpt3",
        "gpt3.5",
        "whisper",
        "api",
        "curl",
        "php-openai",
        "easygithdev"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Florent Brusciano",
            "email": "easygithdev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "EasyGithDev\\PHPOpenAI\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
